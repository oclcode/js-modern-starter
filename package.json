{
  "name": "js-modern-starter",
  "version": "0.1.0",
  "type": "module",
  "description": "A production-ready JavaScript starter template with ESLint v9 (Flat Config), Jest, Babel, and pnpm. Includes ES Modules support, opinionated linting, and pre-configured testing. Ideal for Node.js apps, libraries, and CLI tools.",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "javascript",
    "starter",
    "template",
    "eslint",
    "jest",
    "babel",
    "pnpm",
    "esmodules",
    "modern-js",
    "node"
  ],
  "author": {
    "name": "OCL Code",
    "email": "oclcode@outlook.com",
    "url": "https://github.com/oclcode"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.33.0",
    "babel-jest": "^30.0.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2"
  }
}
